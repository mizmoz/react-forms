{
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "rules": {
        "react/jsx-uses-react": 1,
        "object-shorthand": [2, "consistent"],
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    }
}